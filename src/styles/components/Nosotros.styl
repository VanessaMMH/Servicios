.Nosotros
    background-color white
    padding 50px 130px 
    display: flex
    justify-content: space-between
    h1       
        font-size 40px
        font-weight: 900;
    h4
        font-size 20px  
        font-weight bold
        color #2D4249
        
    strong 
        color #f7fa00
    &-info 
        width: 75%
    &-img 
        
        display: flex
        justify-content: flex-end
        align-items: center
    &-img img  
        border-radius: 5px       
        width: 80%
       
